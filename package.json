{
  "name": "@squareball/cfnspec",
  "version": "0.3.2",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": {
    "email": "gordon@squareball.co",
    "name": "Gordon Leigh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/squareballdigital/cfnspec.git"
  },
  "homepage": "https://github.com/squareballdigital/cfnspec#readme",
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map",
    "!lib/**/*.test.*",
    "src/**/*.ts",
    "!src/**/*.test.ts"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile && npm run compile:cjs && npm run test",
    "compile": "tsc",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "clean": "rm -rf lib/ dist/",
    "lint": "eslint src/ --ext=ts,tsx",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "watch": "tsc -w --incremental"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.2.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fmtk/decoders": "^0.4.3"
  }
}
